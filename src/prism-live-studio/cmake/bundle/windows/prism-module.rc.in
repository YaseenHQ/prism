1 VERSIONINFO
FILEVERSION ${PRISM_VERSION_MAJOR},${PRISM_VERSION_MINOR},${PRISM_VERSION_PATCH},${PRISM_VERSION_TWEAK}
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName", "Naver"
      VALUE "FileDescription", "${MODULE_DESCRIPTION}"
      VALUE "FileVersion", "${PRISM_VERSION}"
      VALUE "ProductName", "PRISMLiveStudio"
      VALUE "ProductVersion", "${PRISM_VERSION}"
      VALUE "Comments", "${PRISM_COMMENTS}"
      VALUE "LegalCopyright", "(C) Naver"
      VALUE "InternalName", "${PROJECT_NAME}"
      VALUE "OriginalFilename", "${PROJECT_NAME}"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 0x04B0
  END
END