
#!/bin/bash

export PROJECT_DIR=@OBS_ENV_PROJECT_DIR@
export BIN_DIR=${PROJECT_DIR}/bin
export SRC_DIR=${PROJECT_DIR}/src

export OBS_SRC_DIR=${PROJECT_DIR}/src/obs-studio
export OBS_BUILD_DIR=${OBS_SRC_DIR}/build
export PRISM_SRC_DIR=${PROJECT_DIR}/src/prism-live-studio
export PRISM_BUILD_DIR=${PRISM_SRC_DIR}/build

export CEF_ROOT_DIR=@OBS_ENV_CEF_ROOT_DIR@
export VLC_DIR=@OBS_ENV_VLC_DIR@
export VIRTUALCAM_GUID=@OBS_ENV_VIRTUALCAM_GUID@
export ALL_DEPS="@OBS_ENV_ALL_DEPS@"

export GENERATOR=@OBS_ENV_GENERATOR@
export COMPILER=@OBS_ENV_COMPILER@
export ARCH=@OBS_ENV_ARCH@
export SUFFIX=@OBS_ENV_SUFFIX@
export VERSION=@OBS_ENV_VERSION@

export BUILD_TYPE=@OBS_ENV_BUILD_TYPE@
export PRISM_VERSION_H_DIR=@OBS_ENV_PRISM_VERSION_H_DIR@
export OBS_SWIG_DIR=@OBS_ENV_SWIG_DIR@
export PRISM_OSX_DEPLOYMENT_TARGET=@OBS_ENV_OSX_DEPLOYMENT_TARGET@

export OUTPUT_DIR=${BIN_DIR}/prism/macos/${BUILD_TYPE}
if [[ ${BUILD_TYPE}=="Debug" ]]; then
	export CMAKE_BUILD_TYPE=Debug
else
	export CMAKE_BUILD_TYPE=RelWithDebInfo
fi