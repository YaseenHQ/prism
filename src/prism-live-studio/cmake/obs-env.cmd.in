set PROJECT_DIR=@OBS_ENV_PROJECT_DIR@
set BIN_DIR=%PROJECT_DIR%/bin
set SRC_DIR=%PROJECT_DIR%/src

set OBS_SRC_DIR=@OBS_ENV_OBS_SRC_DIR@
set OBS_BUILD_DIR=@OBS_ENV_OBS_BUILD_DIR@
set PRISM_SRC_DIR=@OBS_ENV_PRISM_SRC_DIR@
set PRISM_BUILD_DIR=@OBS_ENV_PRISM_BUILD_DIR@

set VIRTUALCAM_GUID=@OBS_ENV_VIRTUALCAM_GUID@
set ALL_DEPS=@OBS_ENV_ALL_DEPS@

set GENERATOR=@OBS_ENV_GENERATOR@
set COMPILER=@OBS_ENV_COMPILER@
set ARCH=@OBS_ENV_ARCH@
set SUFFIX=@OBS_ENV_SUFFIX@
set VERSION=@OBS_ENV_VERSION@
set PLS_VERSION=@PRISM_VERSION@

set BUILD_TYPE=@OBS_ENV_BUILD_TYPE@
set PRISM_VERSION_H_DIR=@OBS_ENV_PRISM_VERSION_H_DIR@

set DEV_OUTPUT_DIR=%BIN_DIR%/prism/windows
set OUTPUT_DIR=%DEV_OUTPUT_DIR%/%BUILD_TYPE%

if "%BUILD_TYPE%"=="Debug" (
	set CMAKE_BUILD_TYPE=Debug
) else (
	set CMAKE_BUILD_TYPE=RelWithDebInfo
)
